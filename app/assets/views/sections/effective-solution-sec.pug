.effective-solution
  .container
    h2 Эффективное решение

    
    each val in effectiveSolution
      .effective-solution__item
        .row
          .col-12.col-md-5.col-lg-6
            div(style="background-image: url('img/landing/"+val.href+"')").effective-solution__img

          .col-12.col-md-7.col-lg-6
            .effective-solution__text-wrap
              h4= val.title
              p= val.text